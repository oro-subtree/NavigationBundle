{% spaceless %}
    {% set data = {
        'title': oro_title_render(),
        'titleSerialized': oro_title_render_serialized(),
        'titleShort': oro_title_render_short(),
        'scripts': script,
        'currentRoute': app.request.attributes.get('_route'),
        'flashMessages': app.session.flashbag.all,
        'history': oro_menu_render('history'),
        'mostviewed': oro_menu_render('mostviewed'),
        'content': content,
        'showPinButton': (
            (app.request.attributes.get('_route') != 'oro_default' or show_pin_button_on_start_page)
            and app.request.attributes.get('exception') is empty
        )
    } %}
    {{ data|json_encode|raw }}
{% endspaceless %}
