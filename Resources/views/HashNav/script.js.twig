<script type="text/javascript">
    $(function() {
        if (Oro.hashNavigationEnabled()) {
            Oro.Registry.setElement("oro.hashnavigation.object", new Oro.Navigation({baseUrl : '{{ app.request.getSchemeAndHttpHost() }}'}));
            if (!Backbone.History.started) {
                Backbone.history.start();
            }
        }
    })
</script>
